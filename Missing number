class Solution:
    def missingNumber(self, nums: List[int]) -> int:
        nums.sort()
        n = len(nums) - 1
        if max(nums) != len(nums):
            return len(nums)
        elif 0 not in nums:
            return 0    
        for i in range(n):
            if (1 + nums[i]) != nums[i+1]:
                return nums[i] + 1
        return -1    
